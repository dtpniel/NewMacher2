{
    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "New Macher",
            "type": "node",
            "request": "launch",
            "protocol": "inspector",
            "program": "${workspaceRoot}/node_modules/nuxt/bin/nuxt",
            "stopOnEntry": false,
            "args": ["dev"],
            "cwd": "${workspaceRoot}",
            "sourceMaps": true,
      
            "env": {
                "NODE_ENV": "development",
                "DEBUG": "nuxt:*,app"
            }
            
          },
          {
            
                "type": "node",
                "request": "attach",
                "name": "Attach to Nuxt",
                "port": 9229
              
          }
    ]
  }